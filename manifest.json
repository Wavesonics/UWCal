{
	"name": "UW Cal",

	"description": "Sync UW calendar to Google",

	"version": "0.2",

	"manifest_version": 2,

	"icons": { "16": "images/icon-16.png", "128": "images/icon-128.png" },
  
	"browser_action": {
		"default_icon": "images/icon-128.png",
		"default_popup": "popup.html"
	},
  
	"permissions":
	[
		"tabs",
		"https://sslvpn.medical.washington.edu/wfc/applications/suitenav/*",
		"downloads"
	]
}